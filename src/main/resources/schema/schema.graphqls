type Query {
    bookById(id: ID): Book
    findConsortiumChainById(id: ID): ConsortiumChain
}

type Book {
    id: ID
    name: String
    pageCount: Int
    author: Author
}

type Author {
    id: ID
    firstName: String
    lastName: String
}

type ConsortiumChain {
    id: ID
    name: String
    championId: Int
    email: String
    description: String
    createAt: String
    updateAt: String
    user: User
    nodeList: [ChainNode]
}

type User {
    id: ID
    name: String
    nick: String
    email: String
    sex: Int
    remark: String
    createAt: String
    updateAt: String
}

type ChainNode {
    id: ID
    userId: Int
    name: String
    chainId: Int
    type: Int
    ip: String
    port: Int
    remark: String
    createAt: String
    updateAt: String
}
